[shell multi]
    !echo xxxabcdefgihyyy
    ?echo xxxabcdefgihyyy
    ?++def
    ?++gih
    ?abc

    !echo xxxabcdefgihyyy
    ?echo xxxabcdefgihyyy
    ?++abc
    ?++def
    ?gih

    !echo xxxabcdefgihyyy
    ?echo xxxabcdefgihyyy
