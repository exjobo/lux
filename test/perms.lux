[shell relax]
    !echo xxxabc111def222gihyyy
    ?echo xxxabc111def222gihyyy
    ?+def
    ?+gih
    ?abc

    !echo xxxabcdefgihyyy
    ?echo xxxabcdefgihyyy
    ?+abc
    ?+def
